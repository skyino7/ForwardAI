USE classicmodels;

CREATE TABLE mytable(
   identifier    INTEGER  NOT NULL PRIMARY KEY
  ,"first name"  VARCHAR(5) NOT NULL
  ,"last name"   VARCHAR(7) NOT NULL
  ,"login email" VARCHAR(17) NOT NULL
);
